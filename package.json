    "eslint": "^1.10.3",
    "eslint-config-cycle": "^3.2.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-cycle": "^1.0.2",
    "eslint-plugin-no-class": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "node-noop": "^1.0.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.12"
  }
