    "@cycle/xstream-run": "^3.1.0",
    "express": "^4.14.0",
    "normalize.css": "^5.0.0",
    "url-mapper": "^1.1.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "xstream": "^6.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-cli": "^6.16.0",
